class Solution {
public:
    int totalNumbers(vector<int>& digits) {
         unordered_set<int>un;
         vector<int>h(10,0);
        for (int d:digits){
            h[d]++;
        }
        for(int x=1;x<=9;x++) {
            if(h[x]==0) continue;
            h[x]--;
            for (int y=0;y<=9;y++) {
                if (h[y]==0) continue;

                h[y]--;
                for (int z=0;z<=8;z+=2) {
                    if(h[z]>0) {
                        int number=x*100+y*10+z;
                         un.insert(number);
                    }
                }
                h[y]++; 
            }
            h[x]++; 
        }

        return un.size();
    }
};Â©leetcode
